{
    "name": "jekyll",
    "displayName": "Jekyll",
    "description": "Author and preview Jekyll site in Visual Studio Code",
    "version": "0.0.1",
    "publisher": "rebornix",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.previewJekyll"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.previewJekyll",
            "title": "Jekyll"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}